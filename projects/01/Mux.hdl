/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel,out=o1);
    Nand(a=a,b=o1,out=o2);
    Nand(a=sel,b=b,out=o3);
    Nand(a=o2,b=o3,out=out);
}